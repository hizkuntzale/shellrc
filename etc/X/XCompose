include "%L"

# Сербский
<Multi_key> <s> <x> : "č"
<Multi_key> <s> <c> : "ć"
<Multi_key> <s> <j> : "đ"
<Multi_key> <s> <g> : "dž"
<Multi_key> <s> <z> : "ž"
<Multi_key> <s> <s> : "š"

<Multi_key> <s> <X> : "Č"
<Multi_key> <s> <C> : "Ć"
<Multi_key> <s> <J> : "Đ"
<Multi_key> <s> <G> : "Dž"
<Multi_key> <s> <Z> : "Ž"
<Multi_key> <s> <Z> : "Š"

<Multi_key> <k> <c> : "ћ"
<Multi_key> <k> <j> : "ђ"
<Multi_key> <k> <g> : "џ"
<Multi_key> <k> <l> : "љ"
<Multi_key> <k> <n> : "њ"

<Multi_key> <k> <C> : "Ћ"
<Multi_key> <k> <J> : "Ђ"
<Multi_key> <k> <G> : "Џ"
<Multi_key> <k> <L> : "Љ"
<Multi_key> <k> <N> : "Њ"

# Испанский

<Multi_key> <n> <asciitilde>		: "ñ"
<Multi_key> <N> <asciitilde>		: "Ñ"
<Multi_key> <exclam> <exclam>    	: "¡"
<Multi_key> <question> <question>	: "¿"

# Румынский

<Multi_key> <s> <comma>			: "ș"
<Multi_key> <S> <comma>			: "Ș"
<Multi_key> <t> <comma>			: "ț"
<Multi_key> <T> <comma>			: "Ț"
                             
<Multi_key> <a> <less>			: "ă"
<Multi_key> <A> <less>			: "Ă"
<Multi_key> <a> <greater>		: "â"
<Multi_key> <A> <greater>		: "Â"
<Multi_key> <T> <greater>		: "î"
<Multi_key> <T> <greater>		: "Î"

# Кавычки

<Multi_key> <less> <less>        	: "«"
<Multi_key> <greater> <greater>  	: "»"
<Multi_key> <less> <apostrophe>  	: "‘"
<Multi_key> <apostrophe> <less>  	: "‘"
<Multi_key> <greater> <apostrophe> 	: "’"
<Multi_key> <apostrophe> <greater> 	: "’"
<Multi_key> <comma> <apostrophe> 	: "‚"
<Multi_key> <apostrophe> <comma> 	: "‚"
<Multi_key> <less> <quotedbl>    	: "“"
<Multi_key> <quotedbl> <less>    	: "“"
<Multi_key> <greater> <quotedbl> 	: "”"
<Multi_key> <quotedbl> <greater> 	: "”"
<Multi_key> <comma> <quotedbl>   	: "„"
<Multi_key> <quotedbl> <comma>   	: "„"

# Разное

<Multi_key> <minus> <minus>		: "–"
